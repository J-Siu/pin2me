{
	"author": "JSiu.Dev",
	"browser_specific_settings": {
		"gecko": {
			"id": "pin2me@jsiu.dev"
		}
	},
	"chrome_url_overrides": {
		"newtab": "your_page.html"
	},
	"content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
	},
	"description": "Pin your website from any device, any browser! Support Import/Export without sign-in.",
	"icons": {
		"128": "icons/pin2me-128.png",
		"512": "icons/pin2me-512.png"
	},
	"manifest_version": 3,
	"name": "Pin2Me",
	"permissions": [
		"identity"
	],
	"short_name": "Pin2Me",
	"version": "2.0.0"
}
